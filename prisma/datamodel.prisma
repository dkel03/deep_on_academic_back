type Test {
  id: ID! @id
  createdAt: DateTime! @createdAt
  type: String!
  description: String! 
  answerSheet: [AnswerSheet!]!
}

type AnswerSheet {
  id: ID! @id
  name: String!
  answers: [Answer!]!
}

type Answer {
  id: ID! @id
  number: Int!
  answer: String!
}

type User {
  id: ID! @id
  name: String!
  email: String! @unique
  password: String!
  userType: String
  logs: [Log!]
}

type Log {
  id: ID! @id
  createdAt: DateTime @createdAt
  user: User!
  test: Test!
  answerSheet: [AnswerSheet!]!
  score: Int!
}